<h1>Productos</h1>
<article>
  <table>
    <tr>
      <th>title</th>
      <th>size</th>
      <th>price</th>
      <th>quantity</th>

    </tr>
    {{#each payload}}
      <tr>
        <td>{{this.title}}</td>
        <td>{{this.size}}</td>
        <td>{{this.price}}</td>
        <td>{{this.quantity}}</td>

      </tr>

    {{/each}}

  </table>
  <p>Pagina actual {{page}}</p>
  <a href="{{prevLink}}" role="button">Anterior</a>
  <a href="{{nextLink}}" role="button">sigiente</a>
</article>