<h1>Carrito</h1>
<article>
  <table>
    <tr>
      <th>Title</th>
      <th>Size</th>
      <th>Price</th>
      <th>Quantity</th>
    </tr>
    {{#each cart.products}}
      <tr>
        <td>{{this.product.title}}</td>
        <td>{{this.product.size}}</td>
        <td>{{this.product.price}}</td>
        <td>{{this.quantity}}</td>
      </tr>
    {{/each}}
  </table>

</article>